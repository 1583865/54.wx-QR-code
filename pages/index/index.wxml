<view class="container-box">
  <view class="img-box">
    <canvas style="width: 686rpx;height: 686rpx;background:#f1f1f1;margin: 0 auto" canvas-id='mycanvas'></canvas>
  </view>
  <form bindsubmit='formSubmit'>
    <view class="input-row">
      <label>网址</label>
      <input name="url" type="text" maxlength="255" placeholder="{{placeholder}}" />
    </view>
    <button form-type="submit" class="mybtn" type="primary">生成二维码</button>
  </form>

</view>